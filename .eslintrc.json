{
    "extends": ["airbnb-base","plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "object-shorthand": "off",
      "spaced-comment": "warn",
      "class-methods-use-this": "off",
      "comma-dangle": ["error", "never"],
      "linebreak-style": ["error", "windows"],
      "no-restricted-syntax": "off",
      "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "max-len":"warn",
    "global-require":"off"
    }
    
  }

